SELECT *
FROM site_performance_norm
WHERE site_id IN (
    SELECT DISTINCT connected_fuze_site_id_5g
    FROM active_customer_performance_norm
    WHERE connected_fuze_site_id_5g IS NOT NULL
      AND (imsi IN (SELECT imsi FROM temp_table) OR mdn IN (SELECT mdn FROM temp_table))
)
OR site_id IN (
    SELECT DISTINCT connected_fuze_site_id_4g
    FROM active_customer_performance_norm
    WHERE connected_fuze_site_id_4g IS NOT NULL
      AND (imsi IN (SELECT imsi FROM temp_table) OR mdn IN (SELECT mdn FROM temp_table))
);
